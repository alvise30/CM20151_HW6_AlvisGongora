#include <stdio.h>
#include <stdlib.h>
#include <math.h>

float fun_r(int cuerpo, int step, int coordenada, int dt, float **r, float **v){
  return r[cuerpo][step][coordenada] + (dt*v[cuerpo][step][coordenada]);
}

float fun_v(int cuerpo, int step, int coordenada, int dt, float **v, float **a){
  return v[cuerpo][step][coordenada] + (dt*a[cuerpo][step][coordenada]);
}
